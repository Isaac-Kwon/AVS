/run/initialize
/control/verbose 1
/tracking/verbose 1
/event/verbose 1
/gps/verbose 2

# Set Gamma particle preset and 24 mm distnace
/AVS/Source/SetParticlePreset 1
/AVS/Source/StandDistance     24. mm

# Set Filename
/AVS/Analysis/SetFilename     ./data/gamma_24mm_1M.root

# run 1M particles 
/run/beamOn 1000000