/run/initialize
/control/verbose 1
/tracking/verbose 1
/event/verbose 1
/gps/verbose 2

# Set gamma particle with alpha profile preset and 14 mm distnace
/AVS/Source/SetParticlePreset 0
/AVS/Source/StandDistance     14. mm
/gps/particle gamma
/gps/ene/mono 59.5409 keV

# Set Filename
/AVS/Analysis/SetFilename     ./data/gamma_alpha_14mm_1M_ecal.root

# run 10k particles 
/run/beamOn 1000000