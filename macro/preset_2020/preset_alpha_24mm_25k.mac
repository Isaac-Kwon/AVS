/run/initialize
/control/verbose 1
/tracking/verbose 1
/event/verbose 1
/gps/verbose 2

# Set Alpha particle preset and 24 mm distnace
/AVS/Source/SetParticlePreset 0
/AVS/Source/StandDistance     24. mm

# Set Filename
/AVS/Analysis/SetFilename     ./data/alpha_24mm_25k.root

# run 10k particles 
/run/beamOn 25000