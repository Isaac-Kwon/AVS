/run/initialize
/control/verbose 1
/tracking/verbose 0
/event/verbose 0
/gps/verbose 1

# Set Gamma particle preset and 24 mm distnace
/AVS/Source/SetParticlePreset 1
/AVS/Source/StandDistance     24. mm

# Set Filename
# /AVS/Analysis/SetFilename     ./data/preset_gamma_24mm_1M.root

/AVS/Analysis/RecordPrimaryOnly 1
/AVS/Analysis/SetRecordSet 100

# run 1M particles 
/run/beamOn 1000000