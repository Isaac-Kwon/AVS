/run/initialize
/control/verbose 1
/tracking/verbose 0
/event/verbose 0
/gps/verbose 1

# Set gamma particle with alpha profile preset and 14 mm distnace
/AVS/Source/SetParticlePreset 0
/AVS/Source/StandDistance     14. mm
/gps/particle gamma
/gps/ene/mono 59.5409 keV

# Set Filename
# /AVS/Analysis/SetFilename     ./data/preset_gamma_alpha_14mm_1M.root

/AVS/Analysis/RecordPrimaryOnly 1
/AVS/Analysis/SetRecordSet 100

# run 10k particles 
/run/beamOn 1000000