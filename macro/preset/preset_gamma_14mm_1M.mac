/run/initialize
/control/verbose 1
/tracking/verbose 1
/event/verbose 1
/gps/verbose 2

# Set Gamma particle preset and 14 mm distnace
/AVS/Source/SetParticlePreset 1
/AVS/Source/StandDistance     14. mm

# Set Filename
# /AVS/Analysis/SetFilename     ./data/preset_gamma_14mm_1M.root

/AVS/Analysis/RecordPrimaryOnly 1
/AVS/Analysis/SetRecordSet 100

# run 1M particles 
/run/beamOn 1000000